{
  "name": "nidia-flow-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r tsconfig-paths/register dist/src/main.js",
    "start:dev": "npm run docker:up && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v --remove-orphans",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:setup:clean": "./scripts/dev-setup.sh --clean",
    "db:generate": "prisma generate --schema=prisma/superadmin-schema.prisma && prisma generate --schema=prisma/tenant-schema.prisma",
    "db:push": "prisma db push --schema=prisma/superadmin-schema.prisma",
    "db:push:tenant": "DATABASE_URL=\"$TENANT_DATABASE_URL\" prisma db push --schema=prisma/tenant-schema.prisma",
    "db:migrate": "prisma migrate dev --schema=prisma/superadmin-schema.prisma",
    "db:migrate:tenant": "DATABASE_URL=\"$TENANT_DATABASE_URL\" prisma migrate dev --schema=prisma/tenant-schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=prisma/superadmin-schema.prisma",
    "db:studio": "prisma studio --schema=prisma/superadmin-schema.prisma",
    "db:studio:tenant": "DATABASE_URL=\"$TENANT_DATABASE_URL\" prisma studio --schema=prisma/tenant-schema.prisma",
    "db:seed": "ts-node scripts/seed.ts",
    "db:reset": "prisma migrate reset --force --schema=prisma/superadmin-schema.prisma",
    "db:reset:tenant": "DATABASE_URL=\"$TENANT_DATABASE_URL\" prisma migrate reset --force --schema=prisma/tenant-schema.prisma",
    "db:setup": "node scripts/setup-prisma.js",
    "db:validate": "ts-node scripts/validate-schema.ts",
    "db:cleanup": "ts-node scripts/cleanup-sessions.ts",
    "validate:setup": "node scripts/validate-setup.js",
    "provision:tenant": "ts-node scripts/provision-tenant.ts"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}